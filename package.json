{
  "name": "dejpeg-server",
  "version": "1.0.0",
  "description": "DeJPEG Image Processing Server",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "build": "tsc && cp -r src/static dist",
    "watch": "tsc -w"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-node": "^1.16.3",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "image-processing",
    "onnx",
    "neural-networks",
    "dejpeg"
  ],
  "author": "jeeneo",
  "license": "GPL-3.0-only",
  "bin": "dist/app.js",
  "pkg": {
    "assets": [
      "dist/static/**/**/*"
    ],
    "scripts": []
  }
}