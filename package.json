{
  "name": "dejpeg-desktop",
  "version": "1.0.1",
  "description": "DeJPEG desktop",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "build": "tsc && cp -r src/static dist",
    "build-linux": "bash scripts/build-linux.sh",
    "build-win": "bash scripts/build-win.sh",
    "watch": "tsc -w"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-node": "^1.16.3",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "author": "jeeneo",
  "license": "GPL-3.0-only",
  "bin": "dist/app.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "node_modules/onnxruntime-node/**/*",
      "node_modules/sharp/**/*"
    ],
    "scripts": []
  }
}