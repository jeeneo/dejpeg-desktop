{
  "name": "dejpeg-desktop",
  "version": "1.0.1",
  "description": "DeJPEG desktop",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "build": "tsc && cp -r src/static dist",
    "build-linux": "bash scripts/build-linux.sh",
    "build-windows": "bash scripts/build-windows.sh",
    "watch": "tsc -w"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.10",
    "onnxruntime-node": "^1.22.0-rev",
    "pkg": "^5.8.1",
    "sharp": "^0.32.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "author": "jeeneo",
  "license": "GPL-3.0-only",
  "bin": "dist/app.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "node_modules/onnxruntime-node/**/*",
      "node_modules/sharp/**/*"
    ],
    "scripts": []
  }
}
